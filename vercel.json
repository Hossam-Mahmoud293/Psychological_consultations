{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/booking",
      "dest": "/booking.html"
    },
    {
      "src": "/payment",
      "dest": "/payment.html"
    },
    {
      "src": "/specialists",
      "dest": "/specialists.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
